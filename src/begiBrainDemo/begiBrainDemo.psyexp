quot;)&amp;#10;    dlg.addText(&quot;Select Modules&quot;)&amp;#10;    &amp;#10;    # Añadir checkboxes para módulos&amp;#10;    for module_id, module_info in modules.items():&amp;#10;        dlg.addField(f&quot;{module_info['name']}:&quot;, module_info[&quot;selected&quot;])&amp;#10;        &amp;#10;        # Añadir tests de cada módulo como checkboxes si el módulo está seleccionado&amp;#10;        for test_id, test_info in module_info[&quot;tests&quot;].items():&amp;#10;            dlg.addField(f&quot;  - {test_info['name']}:&quot;, test_info[&quot;selected&quot;])&amp;#10;    &amp;#10;    # Mostrar el diálogo&amp;#10;    user_input = dlg.show()&amp;#10;    &amp;#10;    if dlg.OK and user_input:&amp;#10;        idx = 0  # Índice para recorrer las respuestas del usuario&amp;#10;        # Actualizar módulos y tests según la entrada del usuario&amp;#10;        for module_id, module_info in modules.items():&amp;#10;            module_info[&quot;selected&quot;] = user_input[idx]&amp;#10;            idx += 1&amp;#10;            for test_id, test_info in module_info[&quot;tests&quot;].items():&amp;#10;                test_info[&quot;selected&quot;] = user_input[idx]&amp;#10;                idx += 1&amp;#10;        return modules&amp;#10;    else:&amp;#10;        print(&quot;User canceled.&quot;)&amp;#10;        return None&amp;#10;&amp;#10;# Uso de la función&amp;#10;selected_modules = show_selection_dialog(modules)&amp;#10;&amp;#10;# Ver los resultados&amp;#10;if selected_modules:&amp;#10;    print(&quot;Selección final:&quot;)&amp;#10;    for module_id, module_info in selected_modules.items():&amp;#10;        print(f&quot;{module_info['name']}: {module_info['selected']}&quot;)&amp;#10;        for test_id, test_info in module_info[&quot;tests&quot;].items():&amp;#10;            print(f&quot;  - {test_info['name']}: {test_info['selected']}&quot;)&amp;#10;'''&amp;#10;&amp;#10;'''&amp;#10;OLD FUNCTIONAL VERSION:&amp;#10;    &amp;#10;    &amp;#10;from psychopy import gui&amp;#10;&amp;#10;# Diccionario inicial&amp;#10;module_selection_info = {'Module_1': False, 'Module_2': False, 'Module_3': False}&amp;#10;&amp;#10;# Checkboxes&amp;#10;dlg = gui.DlgFromDict(dictionary=module_selection_info, &amp;#10;                      title='Experiment Info')  &amp;#10;if dlg.OK:&amp;#10;    print(f&quot;Modules: {module_selection_info}&quot;)&amp;#10;    module_selection_info = {key: int(value) for key, value in module_selection_info.items()}&amp;#10;    print(f&quot;Modules: {module_selection_info}&quot;)&amp;#10;else:&amp;#10;    core.quit()  # Salir si cancelan&amp;#10;'''" valType="extendedCode" updates="constant" name="Before Experiment"/>
