        <Param val="# Definir una lista de puntos [(x, y)] por los que pasará el estímulo&amp;#10;puntos = [(-0.89, 0), (-0.5, 0.3), (0, 0), (0.5, -0.3), (0.89, 0)]&amp;#10;&amp;#10;# Índice del punto actual&amp;#10;indice_punto = 0&amp;#10;&amp;#10;# Posición inicial&amp;#10;moving_stimuli.pos = puntos[indice_punto]&amp;#10;&amp;#10;velocidad = 0.005  # Velocidad del movimiento" valType="extendedCode" updates="constant" name="Begin Experiment"/>
        <Param val="puntos = [[(- 0.89), 0], [(- 0.5), 0.3], [0, 0], [0.5, (- 0.3)], [0.89, 0]];&amp;#10;indice_punto = 0;&amp;#10;moving_stimuli.pos = puntos[indice_punto];&amp;#10;" valType="extendedCode" updates="constant" name="Begin JS Experiment"/>
