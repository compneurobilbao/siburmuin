        <Param val="cube_width = 0.5;&amp;#10;cube_height = 0.5;&amp;#10;cube_x = 0.4;&amp;#10;cube_y = 0;&amp;#10;stim_start_x = (- 0.5);&amp;#10;stim_start_y = 0;&amp;#10;inner_cube_width = cube_width;&amp;#10;inner_cube_height = cube_height;&amp;#10;inner_cube_x = cube_x;&amp;#10;inner_cube_y = cube_y;&amp;#10;outer_cube_width = (cube_width * 1.1);&amp;#10;outer_cube_height = (cube_height * 1.1);&amp;#10;outer_cube_x = cube_x;&amp;#10;outer_cube_y = cube_y;&amp;#10;delta_width_left_side = ((outer_cube_width - inner_cube_width) / 2);&amp;#10;inner_cube_width += delta_width_left_side;&amp;#10;inner_cube_x -= (delta_width_left_side / 2);&amp;#10;" valType="extendedCode" updates="constant" name="Begin JS Experiment"/>
        <Param val="theta_radians = math.radians(angulo);&amp;#10;velocidad_x = (velocidad * Math.cos(theta_radians));&amp;#10;velocidad_y = (velocidad * Math.sin(theta_radians));&amp;#10;stim_x = stim_start_x;&amp;#10;stim_y = stim_start_y;&amp;#10;stim_user_x = stim_start_x;&amp;#10;stim_user_y = stim_start_y;&amp;#10;stim_user_predicted_x = stim_start_x;&amp;#10;stim_user_predicted_y = stim_start_y;&amp;#10;error_dist = (- 1);&amp;#10;error_time = (- 1);&amp;#10;t_hit = (- 1);&amp;#10;t_user = (- 1);&amp;#10;waiting_start = true;&amp;#10;start = false;&amp;#10;hit = false;&amp;#10;stop = false;&amp;#10;position_prediction_made_by_user = false;&amp;#10;t0 = 0;&amp;#10;right_border_x = 0;&amp;#10;top_border_y = 0;&amp;#10;bottom_border_y = 0;&amp;#10;mouse = new psychoJS.eventManager.Mouse({&quot;win&quot;: psychoJS.window});&amp;#10;" valType="extendedCode" updates="constant" name="Begin JS Routine"/>
        <Param val="theta_radians = math.radians(angulo)&amp;#10;velocidad_x = velocidad * math.cos(theta_radians)&amp;#10;velocidad_y = velocidad * math.sin(theta_radians)&amp;#10;&amp;#10;# INITIAL CONFIG &amp; VARIABLE INITIALIZATION&amp;#10;stim_x = stim_start_x&amp;#10;stim_y = stim_start_y&amp;#10;&amp;#10;stim_user_x = stim_start_x&amp;#10;stim_user_y = stim_start_y&amp;#10;&amp;#10;stim_user_predicted_x = stim_start_x&amp;#10;stim_user_predicted_y = stim_start_y&amp;#10;&amp;#10;error_dist = -1&amp;#10;error_time = -1&amp;#10;t_hit = -1&amp;#10;t_user = -1&amp;#10;&amp;#10;waiting_start = True&amp;#10;start = False&amp;#10;hit = False&amp;#10;stop = False&amp;#10;position_prediction_made_by_user = False&amp;#10;&amp;#10;t0=0&amp;#10;right_border_x = 0&amp;#10;top_border_y = 0&amp;#10;bottom_border_y = 0&amp;#10;&amp;#10;# EVENT CONFIGURATION&amp;#10;mouse = event.Mouse(win=win)" valType="extendedCode" updates="constant" name="Begin Routine"/>
        <Param val="Auto-&gt;JS" valType="str" updates="None" name="Code Type"/>
        <Param val="## stim_3: el estimulo controlado por psychopy&amp;#10;## stim_user: el estimulo que mueve el usuario con el raton en el momento de posicionarlo&amp;#10;## stim_user_prediction: el estimulo posicionado en el marco&amp;#10;&amp;#10;text_5.setText(f&quot;ESTADOS: \nWaiting: {waiting_start:.1f}\nStart: {start:.2f}\nHit: {hit:.2f}\nStop: {stop:.2f}\nprediction_made: {position_prediction_made_by_user:.1f}\n\nMETRICAS:\nt0: {t0:.2f}\nt: {t:.2f}\nAngulo: {angulo:.2f}\nError dist: {error_dist:.2f}\nError tiempo: {error_time:.2f}&quot;)&amp;#10;&amp;#10;if waiting_start: # WAITING --&gt; instrucciones + ajustar t0 hasta que empiece la prueba&amp;#10;     text_logs.setText(f&quot;Pulse la barra espaciadora para empezar...&quot;)&amp;#10;     error_dist = -1&amp;#10;     t0 = t&amp;#10;     &amp;#10;elif start: # START --&gt; mover bola&amp;#10;    displacement_x = (t-t0) * velocidad_x&amp;#10;    displacement_y = (t-t0) * velocidad_y&amp;#10;    stim_x = stim_start_x + displacement_x&amp;#10;    stim_y = stim_start_y + displacement_y # + stim_y + &amp;#10;    stim_user.opacity = 0.0&amp;#10;    stim_user_prediction.opacity = 0.0&amp;#10;&amp;#10;elif hit: # HIT (la bola toca el bore pero no se ve) --&gt; velocidad = 0&amp;#10;    velocidad_x = velocidad_y = 0&amp;#10;    &amp;#10;elif stop: # STOP --&gt; now user makes the position prediction&amp;#10;    # Poner el estimulo si y solo si el raton esta en coordenadas del marco&amp;#10;    tolerance = 0.01 # TODO: calcular anchura marco&amp;#10;    mouse_pos = mouse.getPos()&amp;#10;    text_logs.setText(f&quot;Haz click en el borde donde creas que ha impactado la circunferencia...&quot;)&amp;#10;    &amp;#10;    # Condicion para mostrar el estimulo --&gt; solo en los bordes&amp;#10;    if (abs(mouse_pos[0] - right_border_x) &lt;= tolerance) or (abs(mouse_pos[1] - top_border_y) &lt;= tolerance) or (abs(mouse_pos[1] - bottom_border_y) &lt;= tolerance):&amp;#10;        stim_user.opacity = 1.0&amp;#10;        stim_user_x, stim_user_y = mouse_pos[0], mouse_pos[1]&amp;#10;&amp;#10;        # EVENTO CLICK RATON&amp;#10;        buttons = mouse.getPressed()&amp;#10;        if buttons[0]:&amp;#10;            stim_user_predicted_x, stim_user_predicted_y = mouse_pos[0], mouse_pos[1]&amp;#10;            stim_user_prediction.opacity = 1.0&amp;#10;            position_prediction_made_by_user = True&amp;#10;            # Error calculation - euclidean distance between stim_user_prediction and stim_3&amp;#10;            error_dist = ((stim_user_predicted_x - stim_x) ** 2 + (stim_user_predicted_y - stim_y) ** 2) ** 0.5&amp;#10;    else:&amp;#10;        stim_user.opacity = 0  # Ocultar el estímulo si el ratón no está en el centro&amp;#10;     &amp;#10;elif stop and position_prediction_made_by_user:&amp;#10;     text_logs.setText(f&quot;Pulse la barra espaciadora para continuar...&quot;)&amp;#10;&amp;#10;&amp;#10;# Detectar si stim sale de la pantalla&amp;#10;win_width, win_height = win.size  # win es el objeto de ventana en PsychoPy&amp;#10;if stim_x*stim_x &gt; 5 or stim_y*stim_y &gt; 5:&amp;#10;    start = False&amp;#10;    stop = True&amp;#10;&amp;#10;####################################################&amp;#10;##############____EVENTS &amp; STATES____###############&amp;#10;####################################################&amp;#10;    &amp;#10;# START/STOP: Verifica si se ha presionado la tecla&amp;#10;keys = event.getKeys()&amp;#10;if 'space' in keys:&amp;#10;    &amp;#10;    if waiting_start: # Primera pantalla&amp;#10;        start = True&amp;#10;        waiting_start = False&amp;#10;&amp;#10;    elif start: # Evento parar --&gt; users time prediction&amp;#10;        stop = True&amp;#10;        start = False&amp;#10;        t_user = t&amp;#10;        #text_logs.setText(f&quot;&quot;)&amp;#10;        #t_travel += t - t_event&amp;#10;        &amp;#10;    elif stop and position_prediction_made_by_user: # Detectar si ha hecho la predicción&amp;#10;        continueRoutine = False # Fin de la rutina&amp;#10;    else:&amp;#10;        text_logs.setText(f&quot;Debes hacer la predicción de posicion antes de continuar!\n Pide ayuda si la necesitas.&quot;)&amp;#10;&amp;#10;&amp;#10;####################################################&amp;#10;##############____BORDER DETECTION____##############&amp;#10;####################################################&amp;#10;if start:&amp;#10;    &amp;#10;    # Definir los puntos medios de los bordes del área visible de outer_cube&amp;#10;    # Borde superior&amp;#10;    #top_border_x = outer_pos[0]&amp;#10;    #top_border_y = cube_y + outer_cube_height + outer_size[1]/2&amp;#10;    # Borde inferior&amp;#10;    #bottom_border_x = outer_pos[0]&amp;#10;    #bottom_border_y = outer_pos[1] - outer_size[1]/2&amp;#10;    # Borde derecho&amp;#10;    right_border_x = cube_x + outer_cube_width/2 - (outer_cube_width - inner_cube_width)/2&amp;#10;    top_border_y = cube_y + outer_cube_height/2 - (outer_cube_height - inner_cube_height)/4&amp;#10;    bottom_border_y = cube_y - outer_cube_height/2 + (outer_cube_height - inner_cube_height)/4&amp;#10;    left_border_x = cube_x - outer_cube_width/2&amp;#10;&amp;#10;    # Comprobando si stim_3 toca el centro de algún borde&amp;#10;    tolerance = 0.0  # Define qué tan cerca debe estar stim_3 para considerar que toca el borde&amp;#10;&amp;#10;    # Verificar contacto con el borde superior&amp;#10;    if top_border_y - stim_y &lt;= tolerance:&amp;#10;        print(&quot;Contacto con el borde superior&quot;)&amp;#10;        hit = True&amp;#10;&amp;#10;    # Verificar contacto con el borde derecho&amp;#10;    if right_border_x - stim_x &lt;= tolerance: #and abs(stim_3_pos[1] - right_border_y) &lt;= tolerance:&amp;#10;        print(&quot;Contacto con el borde derecho&quot;)&amp;#10;        hit = True&amp;#10;&amp;#10;    # Verificar contacto con el borde inferior&amp;#10;    if  stim_y - bottom_border_y &lt;= tolerance:&amp;#10;        print(&quot;Contacto con el borde inferior&quot;)&amp;#10;        hit = True&amp;#10;        start = False&amp;#10;        &amp;#10;    if stim_x &gt; left_border_x:&amp;#10;        stim_3.opacity = 0.3&amp;#10;    else:&amp;#10;        stim_3.opacity = 1.0" valType="extendedCode" updates="constant" name="Each Frame"/>
        <Param val="var _pj;&amp;#10;function _pj_snippets(container) {&amp;#10;    function in_es6(left, right) {&amp;#10;        if (((right instanceof Array) || ((typeof right) === &quot;string&quot;))) {&amp;#10;            return (right.indexOf(left) &gt; (- 1));&amp;#10;        } else {&amp;#10;            if (((right instanceof Map) || (right instanceof Set) || (right instanceof WeakMap) || (right instanceof WeakSet))) {&amp;#10;                return right.has(left);&amp;#10;            } else {&amp;#10;                return (left in right);&amp;#10;            }&amp;#10;        }&amp;#10;    }&amp;#10;    container[&quot;in_es6&quot;] = in_es6;&amp;#10;    return container;&amp;#10;}&amp;#10;_pj = {};&amp;#10;_pj_snippets(_pj);&amp;#10;text_5.setText(`ESTADOS:&amp;#10;Waiting: ${util.pad(Number.parseFloat(waiting_start).toFixed(1), 1)}&amp;#10;Start: ${util.pad(Number.parseFloat(start).toFixed(2), 1)}&amp;#10;Hit: ${util.pad(Number.parseFloat(hit).toFixed(2), 1)}&amp;#10;Stop: ${util.pad(Number.parseFloat(stop).toFixed(2), 1)}&amp;#10;prediction_made: ${util.pad(Number.parseFloat(position_prediction_made_by_user).toFixed(1), 1)}&amp;#10;&amp;#10;METRICAS:&amp;#10;t0: ${util.pad(Number.parseFloat(t0).toFixed(2), 1)}&amp;#10;t: ${util.pad(Number.parseFloat(t).toFixed(2), 1)}&amp;#10;Angulo: ${util.pad(Number.parseFloat(angulo).toFixed(2), 1)}&amp;#10;Error dist: ${util.pad(Number.parseFloat(error_dist).toFixed(2), 1)}&amp;#10;Error tiempo: ${util.pad(Number.parseFloat(error_time).toFixed(2), 1)}`&amp;#10;);&amp;#10;if (waiting_start) {&amp;#10;    text_logs.setText(`Pulse la barra espaciadora para empezar...`);&amp;#10;    error_dist = (- 1);&amp;#10;    t0 = t;&amp;#10;} else {&amp;#10;    if (start) {&amp;#10;        displacement_x = ((t - t0) * velocidad_x);&amp;#10;        displacement_y = ((t - t0) * velocidad_y);&amp;#10;        stim_x = (stim_start_x + displacement_x);&amp;#10;        stim_y = (stim_start_y + displacement_y);&amp;#10;        stim_user.opacity = 0.0;&amp;#10;        stim_user_prediction.opacity = 0.0;&amp;#10;    } else {&amp;#10;        if (hit) {&amp;#10;            velocidad_x = velocidad_y = 0;&amp;#10;        } else {&amp;#10;            if (stop) {&amp;#10;                tolerance = 0.01;&amp;#10;                mouse_pos = mouse.getPos();&amp;#10;                text_logs.setText(`Haz click en el borde donde creas que ha impactado la circunferencia...`);&amp;#10;                if ((((Math.abs((mouse_pos[0] - right_border_x)) &lt;= tolerance) || (Math.abs((mouse_pos[1] - top_border_y)) &lt;= tolerance)) || (Math.abs((mouse_pos[1] - bottom_border_y)) &lt;= tolerance))) {&amp;#10;                    stim_user.opacity = 1.0;&amp;#10;                    [stim_user_x, stim_user_y] = [mouse_pos[0], mouse_pos[1]];&amp;#10;                    buttons = mouse.getPressed();&amp;#10;                    if (buttons[0]) {&amp;#10;                        [stim_user_predicted_x, stim_user_predicted_y] = [mouse_pos[0], mouse_pos[1]];&amp;#10;                        stim_user_prediction.opacity = 1.0;&amp;#10;                        position_prediction_made_by_user = true;&amp;#10;                        error_dist = Math.pow((Math.pow((stim_user_predicted_x - stim_x), 2) + Math.pow((stim_user_predicted_y - stim_y), 2)), 0.5);&amp;#10;                    }&amp;#10;                } else {&amp;#10;                    stim_user.opacity = 0;&amp;#10;                }&amp;#10;            } else {&amp;#10;                if ((stop &amp;&amp; position_prediction_made_by_user)) {&amp;#10;                    text_logs.setText(`Pulse la barra espaciadora para continuar...`);&amp;#10;                }&amp;#10;            }&amp;#10;        }&amp;#10;    }&amp;#10;}&amp;#10;[win_width, win_height] = psychoJS.window.size;&amp;#10;if ((((stim_x * stim_x) &gt; 5) || ((stim_y * stim_y) &gt; 5))) {&amp;#10;    start = false;&amp;#10;    stop = true;&amp;#10;}&amp;#10;keys = psychoJS.eventManager.getKeys();&amp;#10;if (_pj.in_es6(&quot;space&quot;, keys)) {&amp;#10;    if (waiting_start) {&amp;#10;        start = true;&amp;#10;        waiting_start = false;&amp;#10;    } else {&amp;#10;        if (start) {&amp;#10;            stop = true;&amp;#10;            start = false;&amp;#10;            t_user = t;&amp;#10;        } else {&amp;#10;            if ((stop &amp;&amp; position_prediction_made_by_user)) {&amp;#10;                continueRoutine = false;&amp;#10;            } else {&amp;#10;                text_logs.setText(`Debes hacer la predicción de posicion antes de continuar!&amp;#10;Pide ayuda si la necesitas.`&amp;#10;);&amp;#10;            }&amp;#10;        }&amp;#10;    }&amp;#10;}&amp;#10;if (start) {&amp;#10;    right_border_x = ((cube_x + (outer_cube_width / 2)) - ((outer_cube_width - inner_cube_width) / 2));&amp;#10;    top_border_y = ((cube_y + (outer_cube_height / 2)) - ((outer_cube_height - inner_cube_height) / 4));&amp;#10;    bottom_border_y = ((cube_y - (outer_cube_height / 2)) + ((outer_cube_height - inner_cube_height) / 4));&amp;#10;    left_border_x = (cube_x - (outer_cube_width / 2));&amp;#10;    tolerance = 0.0;&amp;#10;    if (((top_border_y - stim_y) &lt;= tolerance)) {&amp;#10;        console.log(&quot;Contacto con el borde superior&quot;);&amp;#10;        hit = true;&amp;#10;    }&amp;#10;    if (((right_border_x - stim_x) &lt;= tolerance)) {&amp;#10;        console.log(&quot;Contacto con el borde derecho&quot;);&amp;#10;        hit = true;&amp;#10;    }&amp;#10;    if (((stim_y - bottom_border_y) &lt;= tolerance)) {&amp;#10;        console.log(&quot;Contacto con el borde inferior&quot;);&amp;#10;        hit = true;&amp;#10;        start = false;&amp;#10;    }&amp;#10;    if ((stim_x &gt; left_border_x)) {&amp;#10;        stim_3.opacity = 0.3;&amp;#10;    } else {&amp;#10;        stim_3.opacity = 1.0;&amp;#10;    }&amp;#10;}&amp;#10;" valType="extendedCode" updates="constant" name="Each JS Frame"/>
