        <Param val="Both" valType="str" updates="None" name="Code Type"/>
        <Param val="if stop:&amp;#10;    posicion_x = 0&amp;#10;    posicion_y = 0&amp;#10;    &amp;#10;elif detener_bola:&amp;#10;    posicion_x = posicion_x&amp;#10;    posicion_y = posicion_y&amp;#10;    &amp;#10;else: # Calcular posicion segun formula de tiro parabolico&amp;#10;    posicion_x = (t*velocidad_x) + posicion_x&amp;#10;    posicion_y = (t*velocidad_y) - (0.5*g*t*t) + posicion_y&amp;#10;&amp;#10;# Actualiza el componente de texto con la distancia al borde derecho&amp;#10;text_4.setText(f&quot;Angulo disparo: {angulo:.2f}\n Velocidad: {velocidad:.2f}\n Velocidad_x: {velocidad_x:.2f}\n Velocidad_y: {velocidad_y:.2f}\n posicion_x: {posicion_x:.2f}\n posicion_y: {posicion_y:.2f}\nStop: {stop:.2f}\n &quot;)&amp;#10;&amp;#10;&amp;#10;####################################################&amp;#10;###################____EVENTS____###################&amp;#10;####################################################&amp;#10;&amp;#10;# Detiene el estimulo si toca tierra en la caida&amp;#10;if posicion_y &lt; 0:&amp;#10;    detener_bola = True&amp;#10;    &amp;#10;# STOP: Verifica si se ha presionado la tecla Enter (Intro)&amp;#10;keys = event.getKeys()&amp;#10;if 'return' in keys:&amp;#10;    stop = True&amp;#10;    #velocidad_2 = 0&amp;#10;    tunel_3.opacity = 0.3&amp;#10;    stim_2.opacity = 1.0&amp;#10;" valType="extendedCode" updates="constant" name="Each Frame"/>
